<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MacroKey.SelectIP">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center">
            <Entry Placeholder="IP" Keyboard="Telephone" x:Name="IP"/>
            <Button Text="Connect" x:Name="Connect" Pressed="Connect_Pressed"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>